/ {
	/delete-node/ cmem;

	reserved-memory {
		linux,cma@80000000 {
			compatible = "shared-dma-pool";
			reusable;
			reg = <0x00000000 0x90000000 0x0 0x30000000>;
			//        0x0000000040000000-0x00000000bfffffff
			linux,cma-default;
		};
	};
};

